{
  "name": "log-query-interface",
  "version": "2.0.0",
  "description": "Full-stack Log Ingestion and Querying System with analytics dashboard",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build",
    "build:backend": "cd backend && npm install",
    "start": "cd backend && npm start",
    "test": "cd backend && npm test",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "log-ingestion",
    "log-querying",
    "analytics",
    "react",
    "nodejs",
    "express",
    "websocket",
    "real-time"
  ],
  "author": "Goldenmist00",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Goldenmist00/Log-query-interface.git"
  }
}